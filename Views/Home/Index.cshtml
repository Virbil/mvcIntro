<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Portfolio</title>

    <link rel="stylesheet" href="~/css/style.css"/>
    <link rel=”stylesheet” href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark" style="background-color: #333;t">
        <a class="navbar-brand" href="/">Portfolio</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="/registerindex">Sign up</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/loginindex">Log In</a>
                </li>
            </ul>
        </div>
    </nav>

    @{
    var Links = new Dictionary<string, string>();
    Links.Add("Home", "/");
    Links.Add("Projects", "projects");
    Links.Add("Contact", "contact");
        
    foreach(var link in Links)
    {
        <div class="row">
            <div class="col-lg-4">
                <ul>
                    <li><p><a href="@link.Value">@link.Key</a></p></li>
                </ul>
            </div>
        </div>
    }
    
    
    }
    <h1 class="display-4">About Me</h1>
</body>
</html>